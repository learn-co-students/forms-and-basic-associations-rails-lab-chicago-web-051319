<h1>Create a Song</h1>
<%= form_for @song do |f| %>
  <%= f.label("Song Title") %>
    <%= f.text_field :title %>
    <br>
  <%= f.label("Artist Name")%>
    <%= f.text_field :artist_name, list: "artists_autocomplete" %>
    <datalist id="artist_autocomplete">
      <% @artists.each do |artist| %>
          <option value="<%= artist.name %>">
      <%end %>
      </datalist>
  <br>
  <%= f.label("Genre Name") %>
    <%= f.collection_select :genre_id, @genres,:id, :name %>
  <br>
  <input id="song_notes_1" name="song[note_contents][]">
  <br>
  <input id="song_notes_2" name="song[note_contents][]">
  <br>
  <input id="song_notes_3" name="song[note_contents][]">
  <br>
  <input id="song_notes_4" name="song[note_contents][]">
  <br>
  <input id="song_notes_5" name="song[note_contents][]">
  <br>
  <%= f.submit %>
<%end %>