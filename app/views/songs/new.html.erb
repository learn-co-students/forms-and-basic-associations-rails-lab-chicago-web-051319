<h1>Create a Song</h1>

<%= form_for @song  do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.text_field :artist_name, list: "artists_autocomplete"%>
  <datalist id="artists_autocomplete">
    <%@artists.each do |artist|%>
      <option value="<%=artist.name%>">
    <%end%>
  </datalist>
  <br />
  <%=f.text_field :note_contents, id: "song_notes_1", multiple: true %>
  <%=f.text_field :note_contents, id: "song_notes_2", multiple: true %>
  <%=f.text_field :note_contents, id: "song_notes_3", multiple: true %>
  <%=f.text_field :note_contents, id: "song_notes_4", multiple: true %>
  <%=f.text_field :note_contents, id: "song_notes_5", multiple: true %>
  <%=f.collection_select(:genre_id, @genres, :id, :name)%>

  <%= f.submit %>
<% end %>
